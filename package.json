{
  "name": "blocks-svelte-renderer",
  "version": "0.0.3",
  "description": "A Svelte renderer for Strapi's Blocks rich text editor",
  "keywords": [
    "svelte",
    "strapi",
    "blocks",
    "rich-text",
    "renderer",
    "cms",
    "headless-cms"
  ],
  "author": "Tadeáš Maršík <tad.marsik@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmarsik42/blocks-svelte-renderer.git"
  },
  "type": "module",
  "svelte": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "custom-build-beta": "node scripts/build.js",
    "custom-release-beta": "node scripts/release.js"
  },
  "peerDependencies": {
    "svelte": "^5.33.13"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^22",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.33.13",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
